CREATE TABLE Customer (
  ID          int(10) NOT NULL AUTO_INCREMENT, 
  FirstName   varchar(255), 
  LastName    varchar(255), 
  Address     varchar(255), 
  BirthDate   date, 
  PhoneNumber int(10), 
  PRIMARY KEY (ID));
CREATE TABLE Customer_Movie (
  MovieID     int(10) NOT NULL, 
  CustomerID  int(10) NOT NULL, 
  Date_Rented timestamp NULL, 
  Due_Date    timestamp NULL, 
  PRIMARY KEY (MovieID, 
  CustomerID));
CREATE TABLE Movie (
  ID         int(10) NOT NULL AUTO_INCREMENT, 
  ProducerID int(10) NOT NULL, 
  Title      varchar(255), 
  Duration   int(10), 
  Rating     char(1), 
  PRIMARY KEY (ID));
CREATE TABLE Producer (
  ID          int(10) NOT NULL AUTO_INCREMENT, 
  CompanyName varchar(200), 
  Country     varchar(255), 
  PRIMARY KEY (ID));
ALTER TABLE Movie ADD CONSTRAINT FKMovie505371 FOREIGN KEY (ProducerID) REFERENCES Producer (ID);
ALTER TABLE Customer_Movie ADD CONSTRAINT FKCustomer_M14766 FOREIGN KEY (MovieID) REFERENCES Movie (ID);
ALTER TABLE Customer_Movie ADD CONSTRAINT FKCustomer_M797181 FOREIGN KEY (CustomerID) REFERENCES Customer (ID);
